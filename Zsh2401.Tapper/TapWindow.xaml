<Window x:Class="Zsh2401.Tapper.TapWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zsh2401.Tapper"
          xmlns:os="clr-namespace:AutumnBox.Basic.Device.ManagementV2.OS;assembly=AutumnBox.Basic"
        mc:Ignorable="d"
        Title="Tapper控制器" Height="450" Width="800">
    <Window.DataContext>
        <local:VMTapper/>
    </Window.DataContext>
    <Grid>
        <Button Content="text" Command="{Binding Input}" CommandParameter="{x:Static os:AndroidKeyCode.Menu}"/>
    </Grid>
</Window>
